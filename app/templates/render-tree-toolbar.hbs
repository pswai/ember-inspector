<div class="toolbar">
  <button
    type="button"
    class="toolbar__icon-button"
    action={{action 'clearProfiles'}}
    title="Clear"
  ></button>
  <div class="toolbar__search js-render-profiles-search">
    {{input value=searchField placeholder="Search"}}
  </div>
  <div class="filter-bar__pills"></div>

  {{#unless showEmpty}}
    <div class="divider"></div>
    <button class="js-toolbar-page-refresh-btn" {{action "refreshPage"}}>Reload</button>
  {{/unless}}

</div>
